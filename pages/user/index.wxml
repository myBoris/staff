<view class="container">
   <view class="user-header"><!-- 高斯背景图 -->
    <view class="user-bg-box">
      <open-data class="header-bg" type="userAvatarUrl"></open-data>
    </view>
  </view><!-- 高斯背景图 end -->
  <view><!-- 用户信息 -->
    <view class=""><!-- 基本信息 -->
      <view class="user-order-cont dis-flex">
        <view class="p-r" style=" flex:1.4;">
          <view class="header-img-box">
            <open-data type="userAvatarUrl"></open-data>
          </view>
          <open-data class="user-admin onelist-hidden fontcenter" type="userNickName"></open-data>
        </view>
      </view>
    </view><!-- 基本信息 end -->
  </view><!-- 用户信息 end -->
  <view class="m-top20">
  
    <navigator url="../resume/resume">
      <view class="user-order-title dis-flex b-f m-top20">
        <view class="flex-box">
          <text class="iconfont icon-headlines common-icon"></text>
          <text class="user-orderName h4">我的简历</text>
        </view>
        <view class="flex-box">
          <view class="user-orderContBox">
            <text class="iconfont icon-enter"></text>
          </view>
        </view>
      </view>
    </navigator>
    <navigator url="../user/help">
      <view class="user-order-title dis-flex b-f">
        <view class="flex-box">
          <text class="iconfont icon-feedback common-icon"></text>
          <text class="user-orderName h4">帮助中心</text>
        </view>
        <view class="flex-box">
          <view class="user-orderContBox">
            <text class="iconfont icon-enter"></text>
          </view>
        </view>
      </view>
    </navigator>
  </view>

</view>